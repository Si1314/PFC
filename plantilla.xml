
<?xml version="1.0"?>

<funcion tipo="void" nombre="main">
	<param tipo="int" nombre="x"/>
	<body>
		<asignacion nombre="x" valor="1"/>
		<declaracionAsignacion tipo="int" nombre="y" valor="2"/>
		<declaracionAsignacion tipo="int" nombre="z" valor="3"/>
		<if>
			<operadorBinario tipo="bool" operador="<">
				<operando nombre="x"></operando>
				<operando nombre="y"></operando>
			</operadorBinario>
			<then>
			    <operadorBinario tipo="int" operador="=">
			    	<operando nombre="x"></operando>
					<operando nombre="y"></operando>				    	
			    </operadorBinario>
			</then>
			<else>
				<operadorBinario tipo="int" operador="=">
			    	<operando nombre="y"></operando>
					<operando nombre="x"></operando>				    	
			    </operadorBinario>
			</else>
		</if>
	</body>
</funcion>

