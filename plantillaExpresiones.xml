
<?xml version="1.0"?>

<function type="void" name="main">
	<param type="int" name="x"/>
	<body>

		<!-- int y = 2; -->
		<declaration type="int" name="y"/>
		<assignment name="y">
			<constant value="2"></constant>
		</assignment>

		<!-- int z; -->
		<declaration type="int" name="z"/>

		<!-- z = y * 100; -->
		<assignment name="z">
			<binaryOperator type="arithmetic" operator="*">
				<variable name="y"></variable>
				<constant value="100"></constant>
			</binaryOperator>				    	
	    </assignment>

		<!-- y = z + 7 - 3; -->
		<assignment name="y">
			<binaryOperator type="arithmetic" operator="+">
				<variable name="z"></variable>
				<binaryOperator type="arithmetic" operator="-">
					<constant value="7"></constant>
					<constant value="3"></constant>
				</binaryOperator>
			</binaryOperator>				    	
	    </assignment>

		<!-- x = y; -->
		<assignment name="x">
			<variable name="y"></variable>
		</assignment>

		<!-- y = 0; -->
		<assignment name="y">
			<constant value="0"></constant>
		</assignment>
	    
	</body>
</function>

